{
  "name": "@foxer360/auth",
  "version": "0.1.0",
  "description": "Authorization system for the Foxer360 system.",
  "repository": "git@github.com:Foxer-360/auth.git",
  "author": "Filip Such√Ω <nevim42@gmail.com>",
  "license": "MIT",
  "scripts": {
    "add:dashboard": "yarn workspace @foxer360/auth-dashboard add",
    "add:server": "yarn workspace @foxer360/auth-server add",
    "deploy": "yarn workspace @foxer360/auth-server run deploy",
    "docker": "sudo docker-compose up --force-recreate -d",
    "docker:down": "sudo docker-compose down",
    "docker:list": "sudo docker-compose ps",
    "start:dashboard": "yarn workspace @foxer360/auth-dashboard run start",
    "start:server": "yarn workspace @foxer360/auth-server run start"
  },
  "private": true,
  "workspaces": ["dashboard", "server"]
}
