{
  "name": "@foxer360/auth",
  "version": "0.2.4",
  "description": "Authorization system for the Foxer360 system.",
  "repository": "git@github.com:Foxer-360/auth.git",
  "author": "Filip Such√Ω <nevim42@gmail.com>",
  "license": "MIT",
  "scripts": {
    "add:dashboard": "yarn workspace @foxer360/auth-dashboard add",
    "add:server": "yarn workspace @foxer360/auth-server add",
    "deploy": "yarn workspace @foxer360/auth-server run deploy",
    "docker": "docker-compose up --force-recreate -d",
    "docker:down": "docker-compose down",
    "docker:list": "docker-compose ps",
    "prisma:export": "yarn workspace @foxer360/auth-server run prisma:export",
    "prisma:reset": "yarn workspace @foxer360/auth-server run prisma:reset",
    "prisma:seed": "yarn workspace @foxer360/auth-server run prisma:seed",
    "remove:dashboard": "yarn workspace @foxer360/auth-dashboard remove",
    "remove:server": "yarn workspace @foxer360/auth-server remove",
    "start:dashboard": "yarn workspace @foxer360/auth-dashboard run start",
    "start:server": "yarn workspace @foxer360/auth-server run start"
  },
  "private": true,
  "workspaces": ["dashboard", "server"]
}
